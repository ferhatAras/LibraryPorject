@using KutuphaneMvc.Models.Entities
@model List<TBLDUYURU>
@foreach (var x in Model)
{
    <div class="post">
        <div class="user-block">
            <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/photo4.jpg" alt="user image">
            <span class="username">
                <a href="#">@x.KATEGORI</a>
            </span>
            <span class="description">@x.TARIH.ToString()</span>
        </div>
        <!-- /.user-block -->
        <p>
            @x.ICERIK
        </p>
    </div>
}


